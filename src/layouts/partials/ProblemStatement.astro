import { markdownify } from "@/lib/utils/textConverter";
import type { CollectionEntry } from "astro:content";
import { getEntry } from "astro:content";

const data = (
  (await getEntry(
    "problemStatementSection",
    "problem-statement"
  )) as CollectionEntry<"problemStatementSection">
).data;

const { title, subtitle, description, list } = data;
const tagline = (data as any).tagline || "";
const ctaText = (data as any).ctaText || "";
const ctaHref = (data as any).ctaHref || "";

const cards =
  list?.map((item, index) => ({
    name: item.title || "",
    description: item.description || "",
  })) || [];

