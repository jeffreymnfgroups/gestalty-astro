<section class="ps-section relative isolate overflow-hidden bg-light text-body py-8 sm:py-10 lg:py-12">
  <div class="relative z-10 w-full px-4 sm:px-6 lg:px-8 xl:px-12">
    <div class="mx-auto max-w-[1400px]">
      <!-- Header Section -->
      <div class="ps-header text-center mb-12">
          {tagline && (
            <div class="ps-tagline mb-4">
            <p class="text-xs font-bold uppercase tracking-[0.4em] text-secondary">
                {tagline}
              </p>
            </div>
          )}

          {title && (
            <div class="ps-title mb-5">
            <h2 class="text-3xl sm:text-4xl lg:text-[2.9rem] font-bold leading-[1.15] tracking-tight text-[#0d1b3d]" set:html={markdownify(title)} />
            </div>
          )}

          {subtitle && (
            <div class="ps-subtitle mb-6">
            <p class="text-lg sm:text-xl font-semibold text-[#4a5568] leading-relaxed" set:html={markdownify(subtitle)} />
            </div>
          )}

          {description && (
            <div class="ps-description mb-6">
            <p class="text-base sm:text-lg leading-relaxed text-[#2d3748] font-normal" set:html={markdownify(description)} />
                </div>
              )}
            </div>

      <!-- Cards Grid -->
      <div class="ps-media w-full">
        <div class="ps-controls" aria-hidden="false">
          <button class="ps-nav-btn ps-nav-btn--prev" type="button" aria-label="Previous cards">←</button>
          <button class="ps-nav-btn ps-nav-btn--next" type="button" aria-label="Next cards">→</button>
        </div>
        <div class="ps-cards-grid" id="ps-cards-grid">
          {cards.map((card, index) => {
            const colorScheme = cardColors[index % cardColors.length];
            const isDark = colorScheme.bg === "#1C1C1E";
            const icon = cardIcons[index % cardIcons.length];
            return (
              <div
                class="ps-card-container"
                data-card-index={index}
              >
                <div
                  class="ps-card rounded-lg p-6 relative overflow-hidden flex flex-col"
                  style={`background-color: ${colorScheme.bg}; color: ${colorScheme.text};`}
                >
                  {!isDark && (
                    <div
                      class="ps-card-pattern absolute inset-0 opacity-20"
                      style={`background-color: ${colorScheme.pattern}; background-image: repeating-linear-gradient(45deg, transparent, transparent 15px, rgba(0,0,0,0.03) 15px, rgba(0,0,0,0.03) 16px);`}
                    />
                  )}
                  {isDark && (
                    <div
                      class="ps-card-pattern absolute inset-0 opacity-10"
                      style={`background-color: ${colorScheme.pattern}; background-image: repeating-linear-gradient(45deg, transparent, transparent 15px, rgba(255,255,255,0.05) 15px, rgba(255,255,255,0.05) 16px);`}
                    />
                  )}
                  <div class="relative z-10 flex flex-col flex-grow ps-card-body">
                    <div class="ps-card-content mb-6">
                      <h3 class="ps-card-title text-lg sm:text-xl font-bold mb-3 leading-tight text-left ps-card-item" data-translate-z="50" style={`color: ${colorScheme.text};`}>
                        {card.name}
                      </h3>
                      <p class="ps-card-description text-sm sm:text-base leading-relaxed text-left ps-card-item" data-translate-z="60" style={`color: ${colorScheme.desc};`}>
                        {card.description}
                      </p>
                    </div>
                    <div class="ps-card-icon mt-auto flex justify-center items-center ps-card-item" data-translate-z="100">
                      <div class="ps-icon-wrapper" set:html={icon} />
                    </div>
                  </div>
                </div>
              </div>
            );
          })}
        </div>
      </div>
    </div>
  </div>
</section>

<style>
  .ps-header {
    max-width: 800px;
    margin-left: auto;
    margin-right: auto;
  }

  .ps-tagline p {
    color: var(--color-secondary, #fcb23b);
    font-weight: 700;
  }

  .ps-title h2 {
    color: #0d1b3d;
    font-weight: 800;
  }

  .ps-subtitle p {
    color: #4a5568;
    font-weight: 600;
  }

  .ps-description p {
    color: #2d3748;
    font-weight: 400;
    line-height: 1.75;
  }

  .ps-media {
    width: 100%;
    display: flex;
    justify-content: center;
    position: relative;
  }

  .ps-controls {
    position: absolute;
    right: 0;
    top: -3.75rem;
    display: flex;
    gap: 0.5rem;
    z-index: 20;
    pointer-events: none;
    padding-inline: 0.25rem;
  }

  .ps-nav-btn {
    pointer-events: auto;
    background: #0d1b3d;
    color: #ffffff;
    border: none;
    width: 34px;
    height: 34px;
    border-radius: 50%;
    display: grid;
    place-items: center;
    font-size: 0.95rem;
    font-weight: 700;
    box-shadow: 0 10px 25px rgba(0, 0, 0, 0.15);
    transition: transform 0.15s ease, opacity 0.15s ease;
  }

  .ps-nav-btn:disabled {
    opacity: 0.4;
    cursor: not-allowed;
    transform: scale(0.95);
  }

  .ps-nav-btn:not(:disabled):hover {
    transform: translateY(-2px);
  }

  @media (max-width: 639px) {
    .ps-controls {
      top: -3.25rem;
      right: 0.25rem;
    }

    .ps-nav-btn {
      width: 30px;
      height: 30px;
      font-size: 0.9rem;
    }
  }

  .ps-cards-grid {
    width: 100%;
    max-width: none;
    margin: 0 auto;
    display: flex;
    overflow-x: auto;
    scroll-snap-type: x mandatory;
    scroll-behavior: smooth;
    -webkit-overflow-scrolling: touch;
    padding: 0.25rem 0.5rem 0.75rem;
    gap: 1.5rem;
    cursor: grab;
  }

  .ps-card-container {
    perspective: 1000px;
    flex: 0 0 calc(33.333% - 1rem);
    min-width: 210px;
    max-width: 300px;
    height: 300px;
    scroll-snap-align: start;
  }

  .ps-card {
    --ps-icon-size: clamp(78px, 11vw, 110px);
    width: 100%;
    min-height: 300px;
    height: 300px;
    box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
    display: flex;
    flex-direction: column;
    transition: box-shadow 0.2s ease;
    transform-style: preserve-3d;
  }

  .ps-card:hover {
    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04);
  }

  .ps-card-body {
    transform-style: preserve-3d;
  }

  .ps-card-item {
    transition: transform 0.2s ease-linear;
  }

  .ps-card-pattern {
    background-size: 20px 20px;
  }

  .ps-card-content {
    flex: 1;
  }

  .ps-card-title {
    font-weight: 700;
    text-align: left;
  }

  .ps-card-description {
    line-height: 1.6;
    text-align: left;
  }

  .ps-card-icon {
    margin-top: auto;
    padding-top: 1rem;
    min-height: 120px;
    display: flex;
    justify-content: center;
    align-items: center;
  }

  .ps-icon-wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
    width: var(--ps-icon-size);
    height: var(--ps-icon-size);
    flex-shrink: 0;
  }

  .ps-icon-wrapper svg {
    width: 100%;
    height: 100%;
    max-width: none;
  }

  /* Nudge 9th card icon up to avoid cropping without shifting grid */
  .ps-card-container[data-card-index="8"] .ps-card-icon {
    padding-top: -4rem;
  }

  .ps-card-container[data-card-index="8"] .ps-icon-wrapper {
    margin-top: -70px;
  }

  /* Carousel responsiveness */
  @media (max-width: 1023px) {
    .ps-cards-grid {
      gap: 1.25rem;
    }

    .ps-card-container {
      flex-basis: calc(50% - 0.75rem);
      max-width: 400px;
    }
  }

  @media (max-width: 639px) {
    .ps-card-container {
      flex-basis: 100%;
      min-width: 100%;
      max-width: 100%;
    }
  }

  /* Optional subtle scrollbar styling */
  .ps-cards-grid::-webkit-scrollbar {
    display: none;
  }
  .ps-cards-grid {
    scrollbar-width: none;
    -ms-overflow-style: none;
  }

  .ps-media {
    position: relative;
    align-items: flex-start;
  }

  .ps-cards-grid.ps-dragging {
    cursor: grabbing;
  }
</style>

<script is:inline>
  (function() {
    'use strict';
    function init3DCards() {
      var cardContainers = document.querySelectorAll('.ps-card-container');
      
      cardContainers.forEach(function(container) {
        var cardEl = container.querySelector('.ps-card');
        if (!cardEl || !(cardEl instanceof HTMLElement)) return;
        
        var card = cardEl;
        var cardItems = container.querySelectorAll('.ps-card-item');
        var isMouseEntered = false;

        function handleMouseMove(e) {
          var rect = card.getBoundingClientRect();
          var x = e.clientX - rect.left;
          var y = e.clientY - rect.top;
          var centerX = rect.width / 2;
          var centerY = rect.height / 2;
          var rotateX = (y - centerY) / 25;
          var rotateY = (centerX - x) / 25;

          card.style.transform = 'rotateX(' + rotateX + 'deg) rotateY(' + rotateY + 'deg)';

          cardItems.forEach(function(item) {
            if (!(item instanceof HTMLElement)) return;
            var translateZ = item.getAttribute('data-translate-z');
            if (translateZ && isMouseEntered) {
              var zValue = parseInt(translateZ, 10);
              item.style.transform = 'translateZ(' + zValue + 'px)';
            }
          });
        }

        function handleMouseEnter() {
          isMouseEntered = true;
        }

        function handleMouseLeave() {
          isMouseEntered = false;
          card.style.transform = 'rotateX(0deg) rotateY(0deg)';
          cardItems.forEach(function(item) {
            if (item instanceof HTMLElement) {
              item.style.transform = 'translateZ(0px)';
            }
          });
        }

        container.addEventListener('mousemove', handleMouseMove);
        container.addEventListener('mouseenter', handleMouseEnter);
        container.addEventListener('mouseleave', handleMouseLeave);
      });
    }

    function initDragScroll() {
      var grid = document.getElementById('ps-cards-grid');
      if (!grid) return;

      var isDown = false;
      var startX = 0;
      var scrollLeft = 0;

      function getPageX(e) {
        if (e.touches && e.touches.length) return e.touches[0].pageX;
        return e.pageX;
      }

      function handleDown(e) {
        isDown = true;
        startX = getPageX(e);
        scrollLeft = grid.scrollLeft;
        grid.classList.add('ps-dragging');
      }

      function handleMove(e) {
        if (!isDown) return;
        var x = getPageX(e);
        var walk = x - startX;
        grid.scrollLeft = scrollLeft - walk;
        if (e.cancelable !== false) e.preventDefault();
      }

      function handleUp() {
        isDown = false;
        grid.classList.remove('ps-dragging');
      }

      grid.addEventListener('mousedown', handleDown);
      grid.addEventListener('touchstart', handleDown, { passive: true });
      window.addEventListener('mousemove', handleMove, { passive: false });
      window.addEventListener('touchmove', handleMove, { passive: false });
      window.addEventListener('mouseup', handleUp);
      window.addEventListener('touchend', handleUp);
      window.addEventListener('touchcancel', handleUp);
    }

    function initNavButtons() {
      var grid = document.getElementById('ps-cards-grid');
      var prevBtn = document.querySelector('.ps-nav-btn--prev');
      var nextBtn = document.querySelector('.ps-nav-btn--next');
      if (!grid || !prevBtn || !nextBtn) return;

      var defaultGap = 24; // 1.5rem fallback

      function getStep() {
        var card = grid.querySelector('.ps-card-container');
        if (!card) return 320;
        var style = window.getComputedStyle(grid);
        var gap = parseFloat(style.columnGap || style.gap || defaultGap);
        return card.getBoundingClientRect().width + gap;
      }

      function updateDisabled() {
        var maxScroll = grid.scrollWidth - grid.clientWidth - 2;
        prevBtn.disabled = grid.scrollLeft <= 0;
        nextBtn.disabled = grid.scrollLeft >= maxScroll;
      }

      function scrollByStep(direction) {
        var step = getStep();
        grid.scrollBy({ left: direction * step, behavior: 'smooth' });
        setTimeout(updateDisabled, 260);
      }

      prevBtn.addEventListener('click', function() {
        scrollByStep(-1);
      });

      nextBtn.addEventListener('click', function() {
        scrollByStep(1);
      });

      grid.addEventListener('scroll', updateDisabled);
      updateDisabled();
    }

    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', init3DCards);
      document.addEventListener('DOMContentLoaded', initDragScroll);
      document.addEventListener('DOMContentLoaded', initNavButtons);
    } else {
      init3DCards();
      initDragScroll();
      initNavButtons();
    }
  })();
</script>

