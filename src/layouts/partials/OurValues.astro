                <div
                  class="values-card"
                data-aos="fade-up-sm"
                  data-aos-delay={200 + index * 50}
                  style={`background-color: ${config.color}; color: white;`}>
                  <svg class="values-card__bg" width="648px" height="648px" aria-hidden="true">
                    <use href={`#${config.background}`} />
                  </svg>
                  <div class="values-card__header">
                    <div class="values-card__category">
                      <svg class="icon" width="16px" height="16px" aria-hidden="true">
                        <use href="#star-8" />
                      </svg>
                      <span class="values-card__category-icon" style={`color: ${config.color};`}>
                        <svg class="icon" width="16px" height="16px" aria-hidden="true">
                          <use href={`#${config.iconType}`} />
                        </svg>
                      </span>
                    </div>
                  </div>
                  <div class="values-card__content">
                  {item.title && (
                      <h3 class="values-card__title" set:html={markdownify(item.title)} />
                  )}
                  {item.description && (
                      <p class="values-card__description" set:html={markdownify(item.description)} />
                  )}
                </div>
              </div>
              );
            })
          }
        </div>
        <div class="row g-4 justify-center pt-16">
          {
            stats?.map((item, index) => (
              <div
                class="md:col-4 lg:col-4"
                data-aos="fade-up-sm"
                data-aos-delay={(stats.length - index) * 100}>
                <div class="text-center">
                  {item.label && (
                    <p
                      class="mb-3 lg:text-lg/[inherit]"
                      set:html={markdownify(item.label)}
                    />
                  )}
                  {item.value && (
                    <h3
                      class="h2 lg:h1 mb-2"
                      set:html={markdownify(item.value)}
                    />
                  )}
                </div>
              </div>
            ))
          }
