---
import ImageMod from "@/components/ImageMod.astro";
import { markdownify } from "@/lib/utils/textConverter";
import { getEntry, type CollectionEntry } from "astro:content";

const { title, subtitle, description, list, stats } = (
  (await getEntry(
    "ourValuesSection",
    "our-values"
  )) as CollectionEntry<"ourValuesSection">
).data;

// Card colors and backgrounds matching the reference design
const cardConfig = [
  { color: "hsl(4, 56%, 48%)", background: "grass-top-down", iconType: "link" },
  { color: "hsl(158, 43%, 33%)", background: "grass-left", iconType: "people" },
  { color: "hsl(32, 62%, 47%)", background: "curves", iconType: "link" },
  { color: "hsl(320, 60%, 58%)", background: "circles-top", iconType: "ribbon" },
  { color: "hsl(225, 58%, 53%)", background: "circles-corner", iconType: "coil" },
  { color: "hsl(262, 44%, 53%)", background: "grass-bottom", iconType: "ribbon" },
];
---

<style>
  .values-card {
    aspect-ratio: 1 / 1;
    border-radius: 1.25em;
    font-weight: 300;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    overflow: hidden;
    padding: 1.25em;
    width: 100%;
    max-width: 100%;
    position: relative;
    color: white;
  }

  .values-card__header,
  .values-card__content {
    display: flex;
    position: relative;
    color: white;
  }

  .values-card__header {
    gap: 0.75em;
    justify-content: flex-end;
  }

  .values-card__content {
    flex-direction: column;
  }

  .values-card__bg {
    color: currentColor;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: auto;
    opacity: 0.15;
    z-index: 0;
  }

  .values-card__label {
    font-size: 1.25em;
    position: relative;
    z-index: 1;
  }

  .values-card__category {
    flex-shrink: 0;
    position: relative;
    height: 3.755em;
    width: 3.755em;
    z-index: 1;
  }

  .values-card__category-icon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 30%;
    transform: translate(-50%, -50%);
  }

  .values-card__category .icon {
    width: 100%;
  }

  .values-card__title,
  .values-card__description {
    position: relative;
    z-index: 1;
  }

  .values-card__title {
    font-family: "Clash Grotesk", sans-serif;
    font-weight: 700;
    font-size: 2.5em;
    margin: 0 0 0.25em;
    color: white;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    line-clamp: 2;
  }

  .values-card__description {
    font-family: "Satoshi", sans-serif;
    font-weight: 300;
    font-size: 1.5em;
    line-height: 1.3;
    margin-bottom: 1.6em;
    color: white;
    opacity: 0.95;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    line-clamp: 2;
  }

  .values-card-grid {
    display: grid;
    gap: 0.5em;
    padding: 0.75em;
    max-width: 100%;
  }

  .icon {
    display: block;
    width: 1em;
    height: auto;
  }

  @media (min-width: 600px) {
    .values-card-grid {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  @media (min-width: 1024px) {
    .values-card:nth-child(4n + 1),
    .values-card:nth-child(4n + 2) {
      font-size: 0.5em;
    }
    .values-card:nth-child(3) {
      grid-column: span 2;
      grid-row: span 2;
    }
    .values-card:nth-child(4) {
      grid-column: 1 / span 2;
      grid-row: 2 / span 2;
    }
    .values-card:nth-child(6) {
      grid-column: 4;
      grid-row: 3;
    }
    .values-card-grid {
      grid-template-columns: repeat(4, 1fr);
    }
  }
</style>

